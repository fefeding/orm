{
    "compileOnSave": true,
    "compilerOptions": {
      "baseUrl": ".",
      "paths": { "*": ["types/*"] },
      "target": "es2017",
      "module": "commonjs",
      "strict": true,
      "noImplicitAny": false,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "charset": "utf8",
      "allowJs": false,
      "pretty": true,
      "noEmitOnError": false,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "allowUnreachableCode": false,
      "allowUnusedLabels": false,
      "strictPropertyInitialization": false,
      "noFallthroughCasesInSwitch": true,
      "skipLibCheck": true,
      "skipDefaultLibCheck": true,
      "inlineSourceMap": true,
      "importHelpers": true,
      "lib": [
        "dom",
        "es2015",
        "es2016",
        "es2015.iterable",
        "es2015.generator"
      ]
    },
    "exclude": [
    ]
  }